{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run start",
    "devPath": "http://localhost:4200",
    "distDir": "../dist/Sell-App"
  },
  "package": {
    "productName": "Sell App",
    "version": "2.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "window": {
        "all": true
      },
      "fs": {
        "all": true,
        "copyFile": true,
        "createDir": true,
        "readDir": true,
        "readFile": true,
        "removeDir": true,
        "removeFile": true,
        "renameFile": true,
        "scope": [
          "**"
        ],
        "writeFile": true
      },
      "path": {
        "all": true
      },
      "dialog": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/128.png",
        "icons/256.png",
        "icons/512.png",
        "icons/icon.icns",
        "icons/256.ico"
      ],
      "identifier": "com.Musa-Kavak.dev",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "resizable": true,
        "title": "Sell App",
        "decorations": false,
        "width": 800,
        "height": 600
      }
    ]
  }
}